{
	"trailingComma": "none",
	"useTabs": true,
	"semi": false,
	"tabWidth": 2,
	"jsxSingleQuote": true,
	"singleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/app/?(.*)$",
		"^@/process/?(.*)$",
		"^@/pages/?(.*)$",
		"^@/layouts/?(.*)$",
		"^@/widgets/?(.*)$",
		"^@/features/?(.*)$",
		"^@/entity/?(.*)$",
		"^@/shared/?(.*)$",
		"(.*/types)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparations": true,
	"importOrderSortSpecifiers": true
}
